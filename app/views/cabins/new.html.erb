<%= form_with url: '/cabins', method: :post, local: true do |form| %>
    <%= form.label :title%>
    <%= form.text_field :title%>

    <%= form.label :co_ed%>
    <%= form.select :co_ed, options_for_select([true, false]) %>

    <%= form.label :max_guest_count%>
    <%= form.text_field :max_guest_count%>

    <%= form.submit "Create Cabin"%>
<% end %>